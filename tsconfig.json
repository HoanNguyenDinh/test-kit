  "include": ["src", "base", "starter-kit", "0Note/OkayBearsET/useCollection.tsx", "0Note/DoingCollection/b/collectionAttribute.tsx"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "exclude": ["node_modules", "0Note"]
